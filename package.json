{
  "name": "securityfixerbot",
  "version": "1.0.0",
  "private": true,
  "description": "Security bot that will create PR with fixes",
  "author": "pether.sorling@gmail.com <pether.sorling@gmail.com>",
  "license": "ISC",
  "repository": "https://github.com/Hack23/securityfixerbot.git",
  "homepage": "https://github.com/Hack23/securityfixerbot",
  "bugs": "https://github.com/Hack23/securityfixerbot/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc",
    "start": "probot run ./lib/index.js",
    "test": "jest --forceExit --detectOpenHandles --coverage",
    "lint": "eslint './src/**/*.{tsx,ts}'  -f json -o eslint-report.json",
    "lint-fix": "eslint './src/**/*.{tsx,ts}' --fix",
    "audit-dependencies": "audit-ci --config audit-ci.json"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "dependencies": {
    "probot": "10.17.1",
    "test": "0.6.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "14.14.9",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "audit-ci": "^3.1.1",
    "eslint": "7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "jest": "^26.6.3",
    "jest-sonar-reporter": "^2.0.0",
    "nock": "13.0.5",
    "prettier": "^2.2.0",
    "prettier-eslint": "^11.0.0",
    "smee-client": "1.2.2",
    "ts-jest": "^26.4.4",
    "typescript": "4.1.2"
  },
  "engines": {
    "node": ">= 10.13.0"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
